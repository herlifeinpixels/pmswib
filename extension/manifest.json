{
	"manifest_version": 2,
	"name": "PMSWIB",
	"description": "Put My Shit Where it Belongs - This is a download extension for Chrome",
	"icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
	"version": "1.0",
	"permissions": ["downloads"],
	"browser_action": {
		"default_title": "Downloader Thingy",
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	}
}